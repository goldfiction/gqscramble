{
  "name": "gqscramble",
  "version": "1.0.2",
  "description": "A small utility for communication and data distributing",
  "main": "app.coffee",
  "node_version": "18.20.2",
  "repository": "git@git.hub:goldfiction/gqscramble.git",
  "dependencies": {
    "zlib": "1.0.5",
    "blowfish-node": "1.1.4",
    "lzma-native": "8.0.6",
    "seedrandom": "3.0.5"
  },
  "devDependencies": {
    "coffeescript": "2.7.0",
    "nodemon": "3.1.10",
    "ansi": "0.3.1",
    "gqtest": ">=0.0.16"
  },
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; coffee app.coffee",
    "test": "node --openssl-legacy-provider --require coffeescript/register test/test.coffee",
    "test:watch": "nodemon --delay 2 -q -x \"npm test\"",
    "test:slow": "node --openssl-legacy-provider --require coffeescript/register test/test.slow.coffee",
    "optional": " --reporter-option output=report.json ",
    "preinstall": "npm install --no-bin-links --omit=optional --ignore-scripts",
    "install:prod": "npm install --production",
    "git": "git add .; git commit -m \"update\"; echo gitted...",
    "git:push": "git push scm; echo pushed...",
    "git:setup": "git config --global user.name \"glidev5\";git config --global user.email \"glidev5@gmail.com\";git branch --set-upstream-to=scm/master master;"
  },
  "author": "glidev5@gmail.com",
  "license": "MIT"
}