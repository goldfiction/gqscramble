{
  "name": "gqscramble",
  "version": "1.0.0",
  "description": "A small utility for communication and data distributing",
  "main": "app.coffee",
  "node_version": "18.20.2",
  "dependencies": {
    "sjson5":"0.1.7",
    "gqcoffee":"1.0.25",
    "gqdoq":"1.1.1",
    "zlib":"1.0.5",
    "blowfish-node":"1.1.4",
    "lzma-native":""
  },
  "devDependencies":{
    "mocha":"11.7.1",
    "coffeescript":"2.7.0",
    "nodemon":"3.1.10",
    "ansi":""
  },
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; coffee app.coffee",
    "test": "mocha --openssl-legacy-provider --require coffeescript/register --no-color --reporter min -t 20000 test/test.coffee",
    "test:watch":"nodemon --delay 2 -q -x \"npm test\"",
    "optional":" --reporter-option output=report.json ",
    "install:nobin": "npm install --no-bin-links --no-optional --omit=optional --ignore-scripts",
    "install:prod":"npm install --production",
    "git":"git add .; git commit -m \"update\"; echo gitted...",
    "git:setup":"git config --global user.name \"glidev5\";git config --global user.email \"glidev5@gmail.com\""
  },
  "author": "glidev5@gmail.com",
  "license": "Apache-2"
}
